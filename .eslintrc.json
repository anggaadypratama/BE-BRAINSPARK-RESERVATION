{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "import/no-unresolved": 0,
        "no-console": 0,
        "prefer-const": 0,
        "no-unused-vars": 0
    }
}
